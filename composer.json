{
  "name": "mbunge/php-attributes",
  "type": "library",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Marco Bunge",
      "email": "marco_bunge@web.de"
    }
  ],
  "homepage": "https://github.com/mbunge/php-attributes",
  "license": "MIT",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "php vendor/bin/phpunit"
  },
  "scripts-descriptions": {
    "changelog": "Use globally installed Conventional Chnagelog CLI to manage changelog",
    "test": "Run PHPUNIT tests"
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunir/phpunit": ">=9.1.4"
  },
  "autoload": {
    "psr-4": {
      "Mbunge\\PhpAttributes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mbunge\\PhpAttributes\\Tests\\": "tests/"
    }
  },
  "description": "Generics with PHP Attributes"
}
